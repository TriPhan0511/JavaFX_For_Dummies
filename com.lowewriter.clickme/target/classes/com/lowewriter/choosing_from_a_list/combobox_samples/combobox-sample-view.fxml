   <children>
      <Label layoutX="75.0" layoutY="151.0" prefHeight="17.0" prefWidth="130.0" text="Which is your favorite?" />
      <ComboBox fx:id="namesComboBox" editable="true" layoutX="205.0" layoutY="147.0" onAction="#namesComboBox_Change" prefWidth="150.0" visibleRowCount="3" />
      <Button fx:id="okButton" layoutX="365.0" layoutY="147.0" mnemonicParsing="false" onAction="#okButton_Click" prefHeight="25.0" prefWidth="57.0" text="OK" />
      <Label fx:id="messageLabel" layoutX="75.0" layoutY="210.0" text="Label" />
   </children>
</AnchorPane>
